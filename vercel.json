{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python",
            "config": { "includeFiles": ["app/static/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ]
}
